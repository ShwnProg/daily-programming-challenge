using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Day27
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = Factorial(5);

            Console.WriteLine(n);
        }
        public static int Factorial(int n)
        {
            if (n == 1)
                return 1;
            else
                return n * Factorial(n - 1);
        }
    }
}
