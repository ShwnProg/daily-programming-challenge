using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Day14
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Day 14. Find duplicate elements in an array.");

            string[] names = { "Shawn", "Marl", "Amaya", "Anna", "Shawn", "Honey", "Bob" };

            for(int i = 0; i < names.Length; i++)
            {
                for(int j = i + 1; j < names.Length; j++)
                {
                    if(names[i] == names[j])
                    {
                        Console.WriteLine("Duplicate : " +  names[i]);
                    }
                }
            }
        }
    }
}
